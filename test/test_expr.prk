function assertEquals(a, b)
{
    if(a != b) {
        println("error!")
    }   
    else {
        println("ok!")
    }
}

function simple_operators()
{
    assertEquals(1 + 1, 2)
    assertEquals(1 + 2, 3)
    assertEquals(1 + 2 + 3, 6)
    assertEquals(1 + 2 + 3 + 4, 10)
    assertEquals(2 * 2, 4)
    assertEquals(1 * 2 * 3, 6)
    assertEquals(1 + 1 * 2 + 2, 5)
    assertEquals(1 + 2 + 3 * 4 * 5 + 6 + 7, 75)
    assertEquals(2 > 1, true)
    assertEquals(2 < 1, false)
    assertEquals(0 >= 1, false)
    assertEquals(1 >= 1, true)
    assertEquals(2 >= 1, true)
}

function main()
{
    simple_operators()
}
